{
  "manifest_version": 2,
  "version": "1.0.9",
  "name": "syncedTabs",
  "short_name": "syncedTabs",
  "description": "access to pages opend in your devices",
  "homepage_url": "https://github.com/ymkz/syncedTabs",
  "permissions": [
    "tabs",
    "sessions",
    "<all_urls>"
  ],
  "icons": {
    "16": "public/icon16.png",
    "48": "public/icon48.png",
    "128": "public/icon128.png"
  },
  "options_page": "option.html",
  "browser_action": {
    "default_popup": "app.html",
    "default_icon": {
      "19": "public/icon19.png"
    }
  }
}
