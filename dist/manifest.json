{
  "manifest_version": 2,
  "name": "syncedTabs",
  "description": "access an opened pages all your devices",
  "version": "1.0.0",
  "permissions": [
    "tabs",
    "sessions",
    "<all_urls>"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "19": "icons/icon19.png"
    }
  }
}
